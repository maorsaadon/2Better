{
  "name": "firebaseauth",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:unit": "jest --config jest.config.js",
    "test:e2e": "detox test --configuration android.emu",
    "build:e2e": "detox build --configuration android.emu",
    "clean:e2e": "detox clean-framework-cache && detox build-framework-cache"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.1.3",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-community/datetimepicker": "7.6.1",
    "@react-native-community/slider": "4.4.2",
    "@react-native-firebase/app": "^18.9.0",
    "@react-native-firebase/storage": "^18.9.0",
    "@react-navigation/bottom-tabs": "^6.5.12",
    "@react-navigation/native": "^6.1.10",
    "@react-navigation/native-stack": "^6.9.18",
    "@types/react": "~18.2.45",
    "base64-js": "^1.5.1",
    "expo": "^50.0.7",
    "expo-constants": "~15.4.5",
    "expo-device": "~5.9.3",
    "expo-image-picker": "~14.7.1",
    "expo-notifications": "~0.27.6",
    "expo-status-bar": "~1.11.1",
    "firebase": "^10.8.0",
    "ngrx-uml": "^1.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.4",
    "react-native-autocomplete-input": "^5.4.0",
    "react-native-check-box": "^2.1.7",
    "react-native-dropdown-picker": "^5.4.6",
    "react-native-gifted-chat": "^2.4.0",
    "react-native-image-crop-picker": "^0.40.2",
    "react-native-image-picker": "^7.1.0",
    "react-native-ionicons": "^4.6.5",
    "react-native-modal-datetime-picker": "^17.1.0",
    "react-native-paper": "^5.12.3",
    "react-native-picker-select": "^9.0.1",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-vector-icons": "^10.0.3",
    "react-native-web": "~0.19.6",
    "react-navigation": "^5.0.0",
    "scheduler": "^0.23.0",
    "uuid": "^9.0.1",
    "watchman": "^1.0.0",
    "winter": "^0.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@firebase/testing": "^0.20.11",
    "@testing-library/react-native": "^13.0.1",
    "detox": "^20.33.0",
    "firebase-admin": "^13.1.0",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "prettier": "^3.0.0-alpha.6"
  },
  "private": true,
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/e2e/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native|react-native)"
    ]
  },
  "detox": {
    "test-runner": "jest",
    "runner-config": "e2e/config.json",
    "apps": {
      "android": {
        "type": "android.apk",
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "build": "cd android && ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release"
      }
    },
    "devices": {
      "android.emu": {
        "type": "android.emulator",
        "device": {
          "avdName": "Pixel_3a_API_30"
        }
      }
    }
  }
}
